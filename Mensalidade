package academia;

/**
 *
 * @author Turato
 */
public class Mensalidade {
    
    private float valor;
    int codigo_aluno;
    private String data_pagamento;
    private String data_fim;

    /**
     * Get the value of data_fim
     *
     * @return the value of data_fim
     */
    public String getData_fim() {
        return data_fim;
    }

    /**
     * Set the value of data_fim
     *
     * @param data_fim new value of data_fim
     */
    public void setData_fim(String data_fim) {
        this.data_fim = data_fim;
    }


    /**
     * Get the value of data_pagamento
     *
     * @return the value of data_pagamento
     */
    public String getData_pagamento() {
        return data_pagamento;
    }

    /**
     * Set the value of data_pagamento
     *
     * @param data_pagamento new value of data_pagamento
     */
    public void setData_pagamento(String data_pagamento) {
        this.data_pagamento = data_pagamento;
    }


    /**
     * Get the value of codigo_aluno
     *
     * @return the value of codigo_aluno
     */
    public int getCodigo_aluno() {
        return codigo_aluno;
    }

    /**
     * Set the value of codigo_aluno
     *
     * @param codigo_aluno new value of codigo_aluno
     */
    public void setCodigo_aluno(int codigo_aluno) {
        this.codigo_aluno = codigo_aluno;
    }

    /**
     * Get the value of valor
     *
     * @return the value of valor
     */
    public float getValor() {
        return valor;
    }

    /**
     * Set the value of valor
     *
     * @param valor new value of valor
     */
    public void setValor(float valor) {
        this.valor = valor;
    }

}
